<?xml version="1.0" encoding="utf-8" ?>
<Moc-Config>
	<MOC MOC_NAME="UEAbility" MOC_NAME_C="终端能力" Module="DC" ACCESS_CONTROL="" DEST_SVC="custom:ubp_ueability_moc:CreateModule:after" Relation="">
		<Parameter name="ISDN" desc="用户号码" type="string" range="[1,8]" constrain="PRI" mode="display" />
		<Parameter name="Model" desc="终端型号" type="enum" range="0,1" vlist="0:手持终端,1:车载台" mode="display" />
		<Parameter name="SoftwareVersion" desc="终端软件版本号" type="enum" range="0" vlist="0:V200R002" mode="display" />
		<Parameter name="UAppVersion" desc="业软软件版本号" type="enum" range="0" vlist="0:V200R002" mode="display" />
		<Parameter name="PreCamera" desc="前置摄像头规格" type="enum" range="0,1,2,3,4,5" vlist="0:NULL,1:QCIF,2:CIF,3:D1,4:720P,5:1080P" mode="display" />
		<Parameter name="PostCamera" desc="后置摄像头规格" type="enum" range="0,1,2,3,4,5" vlist="0:NULL,1:QCIF,2:CIF,3:D1,4:720P,5:1080P" mode="display" />
		<Parameter name="ExtCamera" desc="外置摄像头规格" type="enum" range="0,1,2,3,4,5" vlist="0:NULL,1:QCIF,2:CIF,3:D1,4:720P,5:1080P" mode="display" />
	</MOC>
</Moc-Config>