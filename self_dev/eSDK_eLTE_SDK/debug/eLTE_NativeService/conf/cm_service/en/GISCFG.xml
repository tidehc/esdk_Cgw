<?xml version="1.0" encoding="utf-8" ?>
<Moc-Config>
	<MOC MOC_NAME="GISCFG" MOC_NAME_C="GIS参数" Module="SDS" ACCESS_CONTROL="add,delete,modify,read" DEST_SVC="" Relation="">
		<Parameter name="ISDN" desc="User number" type="string" range="[1,8]" constrain="PRI" mode="display" />
		<Parameter name="GISRptSwt" desc="GIS report switch" type="enum" range="0,1" vlist="0: ON,1: OFF" default="1" />
		<Parameter name="GISRptPrd" desc="GIS report period" type="enum" range="1,2,5,10,15,30,60" vlist="1: 1s,2: 2s,5: 5s,10: 10s,15: 15s,30: 30s,60: 60s" mode="update" default="30" />
		<Parameter name="GISEvt" desc="GIS report event" type="bitset" range="" vlist="1: Emergency call" default="1" />
		<Parameter name="GISRptDis" desc="GIS report distance" type="int" range="[0,10000]" mode="update" default="0" />
	</MOC>
</Moc-Config>